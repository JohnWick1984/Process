        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:process2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="LaunchButton" Content="Запустить процесс" HorizontalAlignment="Center" VerticalAlignment="Center" Click="LaunchButton_Click"/>
        <TextBlock x:Name="DurationTextBlock" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,20"/>
    </Grid>
</Window>
